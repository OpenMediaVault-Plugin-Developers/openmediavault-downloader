{
	"type": "config",
	"id": "conf.service.downloader.download",
	"title": "Downloader download",
	"queryinfo": {
		"xpath": "//services/downloader/downloads/download",
		"iterable": true,
		"idproperty": "uuid"
	},
	"properties": {
		"uuid": {
			"type": "string",
			"format": "uuidv4"
		},
		"dltype": {
			"type": "string",
			"default": "youtube-dl"
		},
		"filename": {
			"type": "string",
			"default": ""
		},
		"url": {
			"type": "string",
			"default": ""
		},
		"parts": {
			"type": "integer",
			"default": 20,
			"minimum": 1
		},
		"keepvideo": {
			"type": "boolean",
			"default": false
		},
		"format": {
			"type": "string",
			"default": ""
		},
		"sharedfolderref": {
			"type": "string",
			"format": "uuidv4"
		},
		"subtitles": {
			"type": "boolean",
			"default": false
		},
		"delete": {
			"type": "boolean",
			"default": false
		}
	}
}
